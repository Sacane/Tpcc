int choose(int a){

    char c;
    putint(a);
    switch(a){
        c = 'M';
        case 10:
            c = 'T';
            putchar(c);
        case 20:
            c = 'P';
            if(c == 'P'){
                while(a < 100){
                    a = a+20;
                    putint(a);
                    c = a;
                    putchar(c);
                    putchar('\n');
                }
            }
            
        case 90:
            putchar('W');
            putchar('\n');
            
        default:
            c = 'N';
            break;
    }
    putchar(c);
    putchar('\n');
    return 0;
}

int main(void)
{
    int a;
    a = 10;
    choose(a);
    return 0;
}
